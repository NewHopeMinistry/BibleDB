        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 9,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3305,
        "VerseNumber": 9,
        "VerseText": "“Yes it is,” replied some of them. “No it is not,” said others, “but he is like him.” His own statement was, “I am the man.”",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 9,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3306,
        "VerseNumber": 10,
