    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 8,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3264,
        "VerseNumber": 27,
        "VerseText": "They did not perceive that He was speaking to them of the Father.",
