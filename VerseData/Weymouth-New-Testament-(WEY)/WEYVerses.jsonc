    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 6,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3144,
        "VerseNumber": 31,
        "VerseText": "Our forefathers ate the manna in the Desert, as it is written, ‘He gave them bread out of Heaven to eat’.”",
