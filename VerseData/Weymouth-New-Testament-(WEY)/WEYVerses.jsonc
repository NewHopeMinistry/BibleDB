        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7274,
        "VerseNumber": 19,
        "VerseText": "For it is an acceptable thing with God, if, from a sense of duty to Him, a man patiently submits to wrong, when treated unjustly.",
