    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 9,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3309,
        "VerseNumber": 13,
        "VerseText": "They brought him to the Pharisees—the man who had been blind.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 9,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3310,
        "VerseNumber": 14,
