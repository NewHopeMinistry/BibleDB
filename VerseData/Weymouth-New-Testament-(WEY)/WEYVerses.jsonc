    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 3,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3854,
        "VerseNumber": 2,
        "VerseText": "some men were carrying there one who had been lame from birth, whom they were wont to place every day close to the Beautiful Gate (as it was called)",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 3,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3855,
        "VerseNumber": 3,
