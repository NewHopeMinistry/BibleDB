        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 8,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3263,
        "VerseNumber": 26,
        "VerseText": "“Many things I have to speak and to judge concerning you. But He who sent me is true, and the things which I have heard from Him are those which I have come into the world to speak.”",
