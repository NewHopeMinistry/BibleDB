        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 8,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3294,
        "VerseNumber": 57,
        "VerseText": "“You are not yet fifty years old,” cried the Jews, “and have you seen Abraham?”",
