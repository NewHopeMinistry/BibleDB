        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 10,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 4130,
        "VerseNumber": 15,
        "VerseText": "Again a second time a voice was heard which said, “What God has purified, you must not regard as unholy.”",
        "VersionID": 24,
