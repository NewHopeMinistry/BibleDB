        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Mark",
        "BookID": 2,
        "ChapterNumber": 14,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 1642,
        "VerseNumber": 32,
