    {
        "Book": "Mark",
        "BookID": 2,
        "ChapterNumber": 13,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 1596,
        "VerseNumber": 23,
        "VerseText": "But as for yourselves, be on your guard: I have forewarned you of everything.",
        "VersionID": 24,
        "VolumeID": 1
