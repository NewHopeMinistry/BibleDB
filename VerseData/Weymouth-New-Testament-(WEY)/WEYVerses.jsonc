        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7276,
        "VerseNumber": 21,
        "VerseText": "And it is to this you were called; because Christ also suffered on your behalf, leaving you an example so that you should follow in His steps.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7277,
        "VerseNumber": 22,
