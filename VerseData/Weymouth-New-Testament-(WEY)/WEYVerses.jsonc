        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 7,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 4020,
        "VerseNumber": 48,
        "VerseText": "Yet the Most High does not dwell in buildings erected by men’s hands. But, as the Prophet declares,",
        "VersionID": 24,
        "VolumeID": 1
    },
