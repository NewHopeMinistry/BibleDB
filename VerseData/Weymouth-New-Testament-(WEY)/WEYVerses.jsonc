        "VolumeID": 1
    },
    {
        "Book": "2 Timothy",
        "BookID": 16,
        "ChapterNumber": 4,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 6729,
        "VerseNumber": 3,
