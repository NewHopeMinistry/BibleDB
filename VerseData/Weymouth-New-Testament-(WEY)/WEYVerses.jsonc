        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 8,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3262,
        "VerseNumber": 25,
        "VerseText": "“You—who are you?” they asked. “How is it that I am speaking to you at all?” replied Jesus.",
