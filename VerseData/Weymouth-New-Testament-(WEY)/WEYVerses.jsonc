    {
        "Book": "Mark",
        "BookID": 2,
        "ChapterNumber": 13,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 1597,
        "VerseNumber": 24,
        "VerseText": "“At that time, however, after that distress, the sun will be darkened and the moon will not shed her light;",
        "VersionID": 24,
        "VolumeID": 1
