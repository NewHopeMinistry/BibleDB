        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 3,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3856,
        "VerseNumber": 4,
