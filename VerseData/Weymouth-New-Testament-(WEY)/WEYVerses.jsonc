    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 6,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3130,
        "VerseNumber": 17,
        "VerseText": "There they got on board a boat, and pushed off to cross the Lake to Capernaum. By this time it had become dark, and Jesus had not yet joined them.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 6,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3131,
        "VerseNumber": 18,
