        "ChapterNumber": 13,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 1588,
        "VerseNumber": 15,
        "VerseText": "let him who is on the roof not come down and enter the house to fetch anything out of it;",
        "VersionID": 24,
        "VolumeID": 1
    },
