        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7268,
        "VerseNumber": 13,
        "VerseText": "Submit, for the Lord’s sake, to every authority set up by man, whether it be to the Emperor as supreme ruler,",
