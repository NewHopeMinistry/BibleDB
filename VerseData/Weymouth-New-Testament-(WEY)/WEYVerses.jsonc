        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 11,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 4190,
        "VerseNumber": 27,
        "VerseText": "At that time certain Prophets came down from Jerusalem to Antioch,",
        "VersionID": 24,
        "VolumeID": 1
