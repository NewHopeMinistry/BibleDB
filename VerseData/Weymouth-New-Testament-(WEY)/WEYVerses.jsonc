    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 7,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3231,
        "VerseNumber": 47,
        "VerseText": "“Are *you* deluded too?” replied the Pharisees;",
        "VersionID": 24,
