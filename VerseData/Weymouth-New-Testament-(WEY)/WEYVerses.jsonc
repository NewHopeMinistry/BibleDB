        "VolumeID": 1
    },
    {
        "Book": "2 Timothy",
        "BookID": 16,
        "ChapterNumber": 3,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 6718,
        "VerseNumber": 9,
