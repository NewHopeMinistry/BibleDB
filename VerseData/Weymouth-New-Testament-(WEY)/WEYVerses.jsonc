        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 1,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7252,
        "VerseNumber": 22,
        "VerseText": "Now that, through your obedience to the truth, you have purified your souls for cherishing sincere brotherly love, you must love another heartily and fervently.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 1,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7253,
        "VerseNumber": 23,
