    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 6,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3149,
        "VerseNumber": 36,
        "VerseText": "But it is as I have said to you: you have seen me and yet you do not believe.",
