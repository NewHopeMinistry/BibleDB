        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3841,
        "VerseNumber": 36,
