        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Matthew",
        "BookID": 1,
        "ChapterNumber": 24,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 824,
        "VerseNumber": 11,
        "VerseText": "Many false prophets will rise up and lead multitudes astray;",
        "VersionID": 24,
        "VolumeID": 1
    },
