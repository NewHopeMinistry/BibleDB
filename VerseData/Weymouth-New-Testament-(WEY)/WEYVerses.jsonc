        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7275,
        "VerseNumber": 20,
