        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "2 Peter",
        "BookID": 22,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7358,
        "VerseNumber": 2,
        "VerseText": "And in their immoral ways they will have many eager disciples, through whom religion will be brought into disrepute.",
        "VersionID": 24,
