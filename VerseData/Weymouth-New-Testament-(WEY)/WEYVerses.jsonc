    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 4,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3883,
        "VerseNumber": 5,
        "VerseText": "The next day a meeting was held in Jerusalem of their Rulers, Elders, and Scribes,",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 4,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3884,
        "VerseNumber": 6,
        "VerseText": "with Annas the High Priest, Caiaphas, John, Alexander, and the other members of the high-priestly family.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 4,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3885,
