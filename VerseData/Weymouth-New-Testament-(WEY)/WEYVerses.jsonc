        "VolumeID": 1
    },
    {
        "Book": "Matthew",
        "BookID": 1,
        "ChapterNumber": 24,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 838,
        "VerseNumber": 25,
