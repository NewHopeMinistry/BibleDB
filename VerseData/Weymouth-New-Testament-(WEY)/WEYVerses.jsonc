    },
    {
        "Book": "John",
        "BookID": 4,
        "ChapterNumber": 7,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 3232,
        "VerseNumber": 48,
        "VerseText": "“has any one of the Rulers or of the Pharisees believed in him?",
        "VersionID": 24,
        "VolumeID": 1
    },
