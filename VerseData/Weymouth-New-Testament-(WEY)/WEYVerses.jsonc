        "ChapterNumber": 13,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 1591,
        "VerseNumber": 18,
        "VerseText": "“But pray that it may not come in the winter.",
        "VersionID": 24,
        "VolumeID": 1
    },
