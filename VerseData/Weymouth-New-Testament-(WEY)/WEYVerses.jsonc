    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7256,
        "VerseNumber": 1,
        "VerseText": "Rid yourselves therefore of all ill-will and all deceitfulness, of insincerity and envy, and of all evil speaking.",
        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "1 Peter",
        "BookID": 21,
        "ChapterNumber": 2,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 7257,
        "VerseNumber": 2,
