        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Acts",
        "BookID": 5,
        "ChapterNumber": 7,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 4029,
        "VerseNumber": 57,
