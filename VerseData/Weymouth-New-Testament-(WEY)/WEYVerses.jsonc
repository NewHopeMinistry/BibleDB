        "VersionID": 24,
        "VolumeID": 1
    },
    {
        "Book": "Matthew",
        "BookID": 1,
        "ChapterNumber": 24,
        "JesusQuote": 0,
        "ParagraphNumber": 0,
        "VerseID": 827,
        "VerseNumber": 14,
        "VerseText": "And this Good News of the Kingdom shall be proclaimed throughout the whole world to set the evidence before all the Gentiles; and then the End will come.",
        "VersionID": 24,
